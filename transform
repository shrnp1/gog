while true do

local args = {
    [1] = {
        [1] = {
            ["Mode"] = "LegendarySuperSaiyan"
        },
        [2] = "J",
        [3] = "UsedTransform",
        [4] = "\26"
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("dataRemoteEvent"):FireServer(unpack(args))

wait (5)
end


